{
  "version": 2,
  "builds": [
    {
      "src": "main/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "excludeFiles": "/{.next,.cache,node_modules,.git,__pycache__}/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main/wsgi.py"
    }
  ],
  "functions": {
    "*": {
      "excludeFiles": "/{.next,.cache,node_modules,.git,__pycache__}/**"
    }
  }
}